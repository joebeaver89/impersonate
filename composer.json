{
    "name": "thepinecode/impersoante",
    "description": "Impersonate users easily in your Laraver app.",
    "keywords": [
        "laravel",
        "debug",
        "impersoante"
    ],
    "homepage": "https://github.com/thepinecode/impersonate",
    "license": "MIT",
    "authors": [
        {
            "name": "Gergo D. Nagy",
            "email": "hello@iamgergo.com",
            "homepage": "https://iamgergo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Pine\\Impersonate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pine\\Impersonate\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pine\\Impersonate\\ImpersonateServiceProvider"
            ]
        }
    }
}
