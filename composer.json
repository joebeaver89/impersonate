{
    "name": "thepinecode/impersonate",
    "description": "Impersonate users easily in Laravel apps.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gergo D. Nagy",
            "email": "dngege@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pine\\Impersonate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pine\\Impersonate\\Test\\": "tests/"
        }
    },
    "require": {
        "php" : "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "orchestra/testbench":"~3.5.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pine\\Impersonate\\ImpersonateServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
